{
    "api": {
      "version": "v1.0",
      "urls": {
        "base": "https://api.northfast.co.ke",
        "thirdpayty_signin": "https://api.northfast.co.ke/signin",
        "register": "https://api.example.com/register",
        "dashboard": "https://api.example.com/dashboard"
      }
    },
    "features": {
      "2fa": true,
      "passwordStrength": true,
      "biometric": false
    },
    "modules": {
      "signinModule": true,
      "registrationModule": true,
      "preauthModules": [
        "IPCheck",
        "GeoLocationCheck"
      ]
    },
    "dashboardUrl": "https://dashboard.example.com",
    "workers": {
      "backgroundJobWorker": true,
      "notificationWorker": true
    },
    "otherInfo": {
      "supportEmail": "support@northfast.co.ke",
      "maxLoginAttempts": 5,
      "passwordPolicy": {
        "minLength": 8,
        "requireNumbers": true,
        "requireSpecialChars": true
      }
    }
  }

  